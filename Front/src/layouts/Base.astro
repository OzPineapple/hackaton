---
export interface Props {
    titulo: string;
}
const { titulo } = Astro.props
---
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{titulo}</title>
 
</head>
<body>
    
    <slot></slot>
</body>
</html>

<style>
    ul{
        list-style: none;
    }
</style>
